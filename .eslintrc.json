{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "no-tabs":"off",
        "require-jsdoc":"off",
        "linebreak-style":"off",
        "quotes":"off",
        "max-len":"off",
        "no-trailing-spaces":"off",
        "no-unused-vars":"warn",
        "new-cap":"warn"
    }
}